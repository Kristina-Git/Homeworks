//6. For all the reference types - Declare variables. Assign values on the variables.
//7. Print the values of the variables.
//8. Print the type of the variables.
let typesOfanimals1 = {
    amphibians:'frogs',
    birds:'parrots'
}
let typesOfanimals2 = {
    fish:'eel',
    mamals:'elephant'
}

let animals=[typesOfanimals1,typesOfanimals2];
console.log(animals);

console.log(typeof typesOfanimals1);
console.log(typeof typesOfanimals2); 
console.log(typeof animals); //the type is object
